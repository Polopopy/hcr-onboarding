<mujoco model="dog">
  <compiler angle="degree"/>
  <option timestep="0.001" gravity="0 0 -9.81"/>

  <default>
    <equality solref="0.005 1" />
    <geom type="box" size="0.05 0.02 0.02" density="500" rgba="0.2 0.7 0.2 1"/>
    <joint limited="true" range="-300 300" damping="0.2" stiffness="5"/>
    <motor ctrllimited="true" ctrlrange="-20 20"/>
  </default>

  <worldbody>
    <!-- Ground -->
    <geom name="ground" type="plane" size="10 10 0.1" rgba="0.8 0.8 0.8 1"/>

    <!-- Torso -->
    <body name="torso" pos="0 0 0.5">
      <freejoint name="torso_free"/>
      <geom name="torso_geom" type="box" size="0.2 .03 .03" pos="0 0 0"/>

      <!-- Front Left Hip -->
      <body name="fl_hip" pos="0.2 0.05 -0.015">"
        <joint name="fl_hip_pitch" type="hinge" axis="0 1 0" pos="-0.03 0 0"/>
        <geom name="fl_hip_geom" type="box" size="0.05 .02 .02" pos="0 0 0"/>

        <!-- Front Left Upper Leg -->
        <body name="fl_upper_leg" pos="0.01 0.03 -0.06">
          <joint name="fl_upper_pitch" type="hinge" axis="0 1 0" pos="0 0 0.05"/>
          <geom name="fl_upper_geom" type="box" size="0.01 0.01 0.05" pos="0 0 0"/>

          <!-- Front Left Lower Leg -->
          <body name="fl_lower_leg" pos="0 -0.018 -0.09">
            <joint name="fl_knee" type="hinge" axis="0 1 0" pos="0 0 0.05"/>
            <geom name="fl_lower_geom" type="box" size="0.01 0.01 0.05" pos="0 0 0"/>
            
            <!-- Front Left Foot -->
            <body name="fl_foot" pos="0 0 -0.05">
              <joint name="fl_ankle" type="hinge" axis="0 1 0" pos="0 0 0.02"/>
              <geom name="fl_foot_geom" type="sphere" size="0.02" pos="0 0 0"/>
            </body>
          </body>
        </body>
      </body>

      <!-- Front Right Hip -->
      <body name="fr_hip" pos="0.2 -0.05 -0.015">"
        <joint name="fr_hip_pitch" type="hinge" axis="0 1 0" pos="-0.03 0 0"/>
        <geom name="fr_hip_geom" type="box" size="0.05 .02 .02" pos="0 0 0"/>
      </body>

    </body>
  </worldbody>

  <actuator>
    <motor name="fl_hip_motor" joint="fl_hip_pitch" ctrlrange="-20 20"/>
    <motor name="fl_upper_leg_motor" joint="fl_upper_pitch" ctrlrange="-20 20"/>
    <motor name="fl_knee_motor" joint="fl_knee" ctrlrange="-20 20"/>
    <motor name="fl_ankle_motor" joint="fl_ankle" ctrlrange="-20 20"/>

    <motor name="fr_hip_motor" joint="fr_hip_pitch" ctrlrange="-20 20"/>
  </actuator>

</mujoco>